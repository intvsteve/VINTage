<Window x:Class="INTV.Shared.View.ProgramFeaturesEditorDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:view="clr-namespace:INTV.Shared.View"
        mc:Ignorable="d" 
        d:DesignHeight="300" d:DesignWidth="300"
        WindowStyle="None" WindowStartupLocation="CenterOwner" ResizeMode="NoResize"
        SizeToContent="WidthAndHeight" UseLayoutRounding="True" SnapsToDevicePixels="True"
        >
    <Grid>
        <view:RomFeaturesConfiguration x:Name="_romFeaturesConfiguration"/>
    </Grid>
</Window>
